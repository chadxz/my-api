{
  "name": "my-api",
  "author": "Chad McElligott <chad.mcelligott@gmail.com>",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "prettify": "prettier --write \"**/*.js\" \"!coverage/**\"",
    "lint": "eslint .",
    "test": "npm run lint && mocha",
    "cover": "nyc --reporter=html --reporter=text mocha && open coverage/index.html",
    "ci-test": "npm run lint && npm run ci-mocha && npm run ci-upload-coverage",
    "ci-mocha": "nyc mocha",
    "ci-upload-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "dependencies": {
    "async": "^1.4.2",
    "body-parser": "^1.9.0",
    "compression": "^1.1.0",
    "config": "^1.2.3",
    "cookie-parser": "^1.4.0",
    "cors": "^2.4.2",
    "csurf": "^1.8.3",
    "express": "^4.9.5",
    "express-session": "^1.11.3",
    "jade": "^1.11.0",
    "lodash": "^3.10.1",
    "method-override": "^2.2.0",
    "passport": "^0.3.0",
    "passport-local": "^1.0.0",
    "redis": "^2.8.0",
    "request": "^2.83.0"
  },
  "devDependencies": {
    "env-test": "^1.0.0",
    "eslint": "^4.9.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "intelli-espower-loader": "^1.0.1",
    "mocha": "^4.0.1",
    "power-assert": "^1.4.4",
    "prettier": "^1.7.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chadxz/my-api.git"
  },
  "nyc": {
    "temp-directory": "coverage/.nyc_output"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "license": "MIT"
}
